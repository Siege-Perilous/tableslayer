{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT", ".env*"],
      "outputs": [".svelte-kit/**", ".vercel/**"]
    },
    "web#build": {
      "env": [
        "ENV_NAME",
        "GITHUB_PR_NUMBER",
        "TURSO_API_TOKEN",
        "TURSO_APP_DB_URL",
        "TURSO_APP_DB_AUTH_TOKEN",
        "RESEND_TOKEN",
        "RESEND_AUDIENCE_ID",
        "TURSO_GS_PARENT_DB_URL",
        "TURSO_GS_PARENT_DB_AUTH_TOKEN",
        "DEV_EMAIL",
        "BASE_URL",
        "CLOUDFLARE_ACCOUNT_ID",
        "CLOUDFLARE_R2_ACCESS_KEY",
        "CLOUDFLARE_R2_SECRET_KEY",
        "CLOUDFLARE_R2_BUCKET_NAME",
        "SENTRY_DSN",
        "SENTRY_AUTH_TOKEN",
        "BODY_SIZE_LIMIT"
      ],
      "outputs": [".svelte-kit/**", ".vercel/**"]
    },
    "web#start": {
      "env": [
        "ENV_NAME",
        "GITHUB_PR_NUMBER",
        "TURSO_API_TOKEN",
        "TURSO_APP_DB_URL",
        "TURSO_APP_DB_AUTH_TOKEN",
        "RESEND_TOKEN",
        "RESEND_AUDIENCE_ID",
        "TURSO_GS_PARENT_DB_URL",
        "TURSO_GS_PARENT_DB_AUTH_TOKEN",
        "DEV_EMAIL",
        "BASE_URL",
        "CLOUDFLARE_ACCOUNT_ID",
        "CLOUDFLARE_R2_ACCESS_KEY",
        "CLOUDFLARE_R2_SECRET_KEY",
        "CLOUDFLARE_R2_BUCKET_NAME",
        "SENTRY_DSN",
        "SENTRY_AUTH_TOKEN",
        "BODY_SIZE_LIMIT"
      ],
      "persistent": true,
      "cache": false
    },
    "docs#start": {
      "env": ["BASE_URL"],
      "persistent": true,
      "cache": false
    },
    "docs#build": {
      "env": ["BASE_URL"],
      "outputs": [".svelte-kit/**", ".vercel/**"]
    },
    "tsc": {
      "dependsOn": ["^tsc"],
      "outputs": []
    },
    "check": {
      "dependsOn": ["^check"],
      "outputs": []
    },
    "lint": {},
    "test": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
