{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT", ".env*"],
      "outputs": [".svelte-kit/**", ".vercel/**"]
    },
    "web#build": {
      "env": ["TURSO_DATABASE_URL", "TURSO_AUTH_TOKEN", "RESEND_TOKEN"]
    },
    "tsc": {
      "dependsOn": ["^tsc"],
      "outputs": []
    },
    "lint": {},
    "test": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
