{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT", ".env*"],
      "outputs": [".svelte-kit/**", ".vercel/**"]
    },
    "web#build": {
      "env": [
        "TURSO_APP_DB_URL",
        "TURSO_APP_DB_AUTH_TOKEN",
        "TURSO_GS_PARENT_DB_URL",
        "TURSO_GS_PARENT_DB_AUTH_TOKEN",
        "RESEND_TOKEN",
        "CLOUDINARY_CLOUD_NAME",
        "CLOUDINARY_URL"
      ]
    },
    "tsc": {
      "dependsOn": ["^tsc"],
      "outputs": []
    },
    "lint": {},
    "test": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
