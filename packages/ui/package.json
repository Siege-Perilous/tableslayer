{
  "name": "@tableslayer/ui",
  "version": "0.0.1",
  "scripts": {
    "dev": "typedoc && pnpm check:watch",
    "build": "svelte-kit sync && vite build && npm run package",
    "preview": "vite preview",
    "tsc": "tsc",
    "package": "svelte-kit sync && svelte-package && typedoc",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "typedoc": "typedoc",
    "postinstall": "svelte-kit sync && pnpm run package"
  },
  "exports": {
    ".": {
      "types": "./src/lib/index.d.ts",
      "svelte": "./src/lib/index.ts"
    },
    "./styles/*.css": {
      "import": "./src/lib/styles/*.css",
      "require": "./src/lib/styles/*.css"
    },
    "./typedoc.json": {
      "import": "./typedocgen.json",
      "require": "./typedocgen.json"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "@melt-ui/svelte": "^0.83.0",
    "@sveltejs/kit": "^2.7.3",
    "svelte": "^5.0.3"
  },
  "devDependencies": {
    "@magidoc/plugin-svelte-marked": "^6.2.0",
    "@melt-ui/svelte": "^0.86.2",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-vercel": "^5.4.8",
    "@sveltejs/kit": "^2.8.3",
    "@sveltejs/package": "^2.3.7",
    "@sveltejs/vite-plugin-svelte": "^4.0.1",
    "@tableslayer/eslint-config": "workspace:*",
    "@tableslayer/tsconfig": "workspace:*",
    "@types/chroma-js": "^2.4.4",
    "@types/marked": "^6.0.0",
    "@types/three": "0.170.0",
    "marked": "^15.0.2",
    "svelte": "5.2.7",
    "svelte-check": "^4.1.0",
    "typedoc": "^0.26.11",
    "typescript": "^5.7.2",
    "vite": "^5.4.11"
  },
  "dependencies": {
    "@tabler/icons-svelte": "^3.22.0",
    "@threlte/core": "8.0.0-next.21",
    "@threlte/extras": "9.0.0-next.28",
    "chroma-js": "^3.1.2",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.5.1",
    "formsnap": "^1.0.1",
    "postprocessing": "6.36.4",
    "shiki": "^1.23.1",
    "three": "0.170.0"
  },
  "svelte": "./src/lib/index.ts",
  "types": "./src/lib/index.d.ts",
  "type": "module"
}
