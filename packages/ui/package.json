{
  "name": "@tableslayer/ui",
  "version": "0.0.1",
  "scripts": {
    "dev": "pnpm check:watch",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && typedoc",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "typedoc": "typedoc"
  },
  "exports": {
    ".": {
      "types": "./src/lib/index.d.ts",
      "svelte": "./src/lib/index.ts"
    },
    "./styles/*.css": {
      "import": "./src/lib/styles/*.css",
      "require": "./src/lib/styles/*.css"
    },
    "./typedoc.json": {
      "import": "./typedocgen.json",
      "require": "./typedocgen.json"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^5.0.0-next.1"
  },
  "devDependencies": {
    "@magidoc/plugin-svelte-marked": "^6.0.1",
    "@melt-ui/svelte": "^0.83.0",
    "@sveltejs/adapter-auto": "^3.2.5",
    "@sveltejs/kit": "^2.5.28",
    "@sveltejs/package": "^2.3.5",
    "@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
    "@tableslayer/eslint-config": "workspace:*",
    "@tableslayer/tsconfig": "workspace:*",
    "@types/marked": "^6.0.0",
    "marked": "^14.1.2",
    "svelte": "5.0.0-next.243",
    "svelte-check": "^4.0.2",
    "typedoc": "^0.26.7",
    "typescript": "^5.6.2",
    "vite": "^5.4.7"
  },
  "dependencies": {
    "@cloudinary/url-gen": "^1.21.0",
    "@tabler/icons-svelte": "^3.17.0",
    "@threlte/core": "8.0.0-next.15",
    "@threlte/extras": "9.0.0-next.22",
    "classnames": "^2.5.1",
    "shiki": "^1.18.0"
  },
  "svelte": "./src/lib/index.ts",
  "types": "./src/lib/index.d.ts",
  "type": "module"
}
