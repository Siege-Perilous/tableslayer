{
  "name": "@tableslayer/ui",
  "version": "0.0.1",
  "scripts": {
    "dev": "typedoc && pnpm check:watch",
    "build": "svelte-kit sync && vite build && npm run package",
    "preview": "vite preview",
    "tsc": "tsc",
    "package": "svelte-kit sync && svelte-package && typedoc",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "typedoc": "typedoc",
    "postinstall": "svelte-kit sync && pnpm run package"
  },
  "exports": {
    ".": {
      "types": "./src/lib/index.d.ts",
      "svelte": "./src/lib/index.ts"
    },
    "./styles/*.css": {
      "import": "./src/lib/styles/*.css",
      "require": "./src/lib/styles/*.css"
    },
    "./typedoc.json": {
      "import": "./typedocgen.json",
      "require": "./typedocgen.json"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "@melt-ui/svelte": "^0.86.2",
    "@sveltejs/kit": "^2.16.0",
    "svelte": "^5.19.0"
  },
  "devDependencies": {
    "@magidoc/plugin-svelte-marked": "^6.2.0",
    "@melt-ui/svelte": "^0.86.6",
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/kit": "^2.37.1",
    "@sveltejs/package": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.4",
    "@tableslayer/eslint-config": "workspace:*",
    "@tableslayer/tsconfig": "workspace:*",
    "@types/chroma-js": "^3.1.1",
    "@types/marked": "^6.0.0",
    "@types/three": "0.180.0",
    "marked": "^16.2.1",
    "svelte": "5.38.7",
    "svelte-check": "^4.3.1",
    "typedoc": "^0.28.12",
    "typescript": "^5.9.2",
    "vite": "^7.1.4"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "@tabler/icons-svelte": "^3.34.1",
    "@threlte/core": "8.1.5",
    "@threlte/extras": "9.5.4",
    "@tiptap/core": "^3.4.1",
    "@tiptap/extension-link": "^3.4.1",
    "@tiptap/extension-typography": "^3.4.1",
    "@tiptap/pm": "^3.4.1",
    "@tiptap/starter-kit": "^3.4.1",
    "chroma-js": "^3.1.2",
    "class-variance-authority": "^0.7.1",
    "lazy-brush": "^2.0.2",
    "postprocessing": "6.37.7",
    "shiki": "^3.12.2",
    "three": "0.180.0",
    "uuid": "^12.0.0",
    "zod": "^4.1.5"
  },
  "svelte": "./src/lib/index.ts",
  "types": "./src/lib/index.d.ts",
  "type": "module"
}
