{
  "name": "@tableslayer/ui",
  "version": "0.0.1",
  "scripts": {
    "dev": "typedoc && pnpm check:watch",
    "build": "svelte-kit sync && vite build && npm run package",
    "preview": "vite preview",
    "tsc": "tsc",
    "package": "svelte-kit sync && svelte-package && typedoc",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "typedoc": "typedoc",
    "postinstall": "svelte-kit sync && pnpm run package"
  },
  "exports": {
    ".": {
      "types": "./src/lib/index.d.ts",
      "svelte": "./src/lib/index.ts"
    },
    "./styles/*.css": {
      "import": "./src/lib/styles/*.css",
      "require": "./src/lib/styles/*.css"
    },
    "./typedoc.json": {
      "import": "./typedocgen.json",
      "require": "./typedocgen.json"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "@melt-ui/svelte": "^0.86.2",
    "@sveltejs/kit": "^2.16.0",
    "svelte": "^5.19.0"
  },
  "devDependencies": {
    "@magidoc/plugin-svelte-marked": "^6.2.0",
    "@melt-ui/svelte": "^0.86.6",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.20.8",
    "@sveltejs/package": "^2.3.11",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tableslayer/eslint-config": "workspace:*",
    "@tableslayer/tsconfig": "workspace:*",
    "@types/chroma-js": "^3.1.1",
    "@types/marked": "^6.0.0",
    "@types/three": "0.176.0",
    "marked": "^15.0.11",
    "svelte": "5.28.2",
    "svelte-check": "^4.1.7",
    "typedoc": "^0.28.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.0",
    "@tabler/icons-svelte": "^3.31.0",
    "@threlte/core": "8.0.4",
    "@threlte/extras": "9.2.1",
    "@tiptap/core": "^2.12.0",
    "@tiptap/extension-link": "^2.12.0",
    "@tiptap/extension-typography": "^2.12.0",
    "@tiptap/pm": "^2.12.0",
    "@tiptap/starter-kit": "^2.12.0",
    "chroma-js": "^3.1.2",
    "class-variance-authority": "^0.7.1",
    "postprocessing": "6.37.3",
    "shiki": "^3.4.0",
    "three": "0.176.0",
    "zod": "^3.24.4"
  },
  "svelte": "./src/lib/index.ts",
  "types": "./src/lib/index.d.ts",
  "type": "module"
}
