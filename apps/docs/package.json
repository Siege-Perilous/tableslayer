{
  "name": "docs",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev-local": "vite dev --host 0.0.0.0 --port 8080",
    "build": "svelte-kit sync && vite build",
    "preview": "vite preview",
    "start": "vite preview --host 0.0.0.0 --port 8080",
    "tsc": "svelte-kit sync && tsc",
    "test": "pnpm run test:integration && pnpm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "postinstall": "svelte-kit sync"
  },
  "dependencies": {
    "@tabler/icons-svelte": "^3.36.1",
    "@tableslayer/ui": "workspace:*",
    "mode-watcher": "^1.1.0",
    "svelte-tweakpane-ui": "^1.5.13"
  },
  "devDependencies": {
    "@melt-ui/pp": "^0.3.2",
    "@playwright/test": "^1.58.0",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.50.1",
    "@sveltejs/vite-plugin-svelte": "6.2.4",
    "@tableslayer/eslint-config": "workspace:*",
    "@tableslayer/tsconfig": "workspace:*",
    "@threlte/core": "8.3.1",
    "@threlte/extras": "9.7.1",
    "@tiptap/core": "^3.17.1",
    "dotenv": "^17.2.3",
    "postprocessing": "6.38.2",
    "svelte": "5.48.2",
    "svelte-check": "^4.3.5",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
