{
  "name": "docs",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build",
    "preview": "vite preview",
    "start": "vite preview --host 0.0.0.0 --port 8080",
    "tsc": "svelte-kit sync && tsc",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "postinstall": "svelte-kit sync"
  },
  "dependencies": {
    "@tabler/icons-svelte": "^3.26.0",
    "@tableslayer/ui": "workspace:*",
    "classnames": "^2.5.1",
    "mode-watcher": "^0.5.0",
    "svelte-tweakpane-ui": "^1.5.1"
  },
  "devDependencies": {
    "@melt-ui/pp": "^0.3.2",
    "@playwright/test": "^1.49.1",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-static": "^3.0.6",
    "@sveltejs/kit": "^2.11.1",
    "@sveltejs/vite-plugin-svelte": "5.0.2",
    "@tableslayer/eslint-config": "workspace:*",
    "@tableslayer/tsconfig": "workspace:*",
    "@threlte/core": "8.0.0-next.15",
    "dotenv": "^16.4.7",
    "svelte": "5.14.0",
    "svelte-check": "^4.1.1",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vitest": "^2.1.8"
  }
}
