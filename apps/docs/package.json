{
  "name": "docs",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build",
    "preview": "vite preview",
    "start": "vite preview --host 0.0.0.0 --port 8080",
    "tsc": "svelte-kit sync && tsc",
    "test": "pnpm run test:integration && pnpm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "postinstall": "svelte-kit sync"
  },
  "dependencies": {
    "@tabler/icons-svelte": "^3.33.0",
    "@tableslayer/ui": "workspace:*",
    "mode-watcher": "^1.0.7",
    "svelte-tweakpane-ui": "^1.5.6"
  },
  "devDependencies": {
    "@melt-ui/pp": "^0.3.2",
    "@playwright/test": "^1.52.0",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.21.1",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "@tableslayer/eslint-config": "workspace:*",
    "@tableslayer/tsconfig": "workspace:*",
    "@threlte/core": "8.0.4",
    "@threlte/extras": "9.2.1",
    "@tiptap/core": "^2.12.0",
    "dotenv": "^16.5.0",
    "postprocessing": "6.37.3",
    "svelte": "5.30.2",
    "svelte-check": "^4.2.1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  }
}
