{
  "name": "docs",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build",
    "preview": "vite preview",
    "start": "vite preview --host 0.0.0.0 --port 8080",
    "tsc": "svelte-kit sync && tsc",
    "test": "pnpm run test:integration && pnpm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "postinstall": "svelte-kit sync"
  },
  "dependencies": {
    "@tabler/icons-svelte": "^3.31.0",
    "@tableslayer/ui": "workspace:*",
    "mode-watcher": "^0.5.1",
    "svelte-tweakpane-ui": "^1.5.6"
  },
  "devDependencies": {
    "@melt-ui/pp": "^0.3.2",
    "@playwright/test": "^1.51.1",
    "@sveltejs/adapter-auto": "^5.0.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.20.2",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "@tableslayer/eslint-config": "workspace:*",
    "@tableslayer/tsconfig": "workspace:*",
    "@threlte/core": "8.0.1",
    "@threlte/extras": "9.1.0",
    "@tiptap/core": "^2.11.5",
    "dotenv": "^16.4.7",
    "postprocessing": "6.37.1",
    "svelte": "5.25.3",
    "svelte-check": "^4.1.5",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vitest": "^3.0.9"
  }
}
