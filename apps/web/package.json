{
  "name": "web",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build",
    "preview": "vite preview",
    "tsc": "svelte-kit sync && tsc",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "appDb:generate": "drizzle-kit generate --config drizzle.config.app.ts",
    "appDb:migrate": "drizzle-kit migrate --config drizzle.config.app.ts",
    "appDb:push": "drizzle-kit push --config drizzle.config.app.ts",
    "appDb:drop": "drizzle-kit drop --config drizzle.config.app.ts",
    "appDb:studio": "drizzle-kit studio --config drizzle.config.app.ts",
    "gsDb:generate": "drizzle-kit generate --config drizzle.config.gs.ts",
    "gsDb:migrate": "drizzle-kit migrate --config drizzle.config.gs.ts",
    "gsDb:push": "drizzle-kit push --config drizzle.config.gs.ts",
    "gsDb:drop": "drizzle-kit drop --config drizzle.config.gs.ts",
    "gsDb:studio": "drizzle-kit studio --config drizzle.config.gs.ts",
    "postinstall": "svelte-kit sync"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.703.0",
    "@libsql/client": "^0.14.0",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@melt-ui/pp": "^0.3.2",
    "@node-rs/argon2": "^2.0.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@tabler/icons-svelte": "^3.23.0",
    "@tableslayer/ui": "workspace:*",
    "@tanstack/svelte-query": "^5.62.0",
    "@tursodatabase/api": "^1.9.0",
    "@types/chroma-js": "^2.4.4",
    "axios": "^1.7.8",
    "chroma-js": "^3.1.2",
    "classnames": "^2.5.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.36.4",
    "drizzle-zod": "^0.5.1",
    "formsnap": "^1.0.1",
    "mime-types": "^2.1.35",
    "mode-watcher": "^0.5.0",
    "paneforge": "^0.0.6",
    "resend": "^4.0.1",
    "slugify": "^1.6.6",
    "sveltekit-superforms": "^2.21.0",
    "uuid": "^11.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-vercel": "^5.5.0",
    "@sveltejs/kit": "^2.9.0",
    "@sveltejs/vite-plugin-svelte": "5.0.1",
    "@tableslayer/eslint-config": "workspace:*",
    "@tableslayer/tsconfig": "workspace:*",
    "@types/mime-types": "^2.1.4",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.28.1",
    "lucia": "^3.2.2",
    "svelte": "5.3.1",
    "svelte-check": "^4.1.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.2",
    "vitest": "^2.1.6"
  }
}
