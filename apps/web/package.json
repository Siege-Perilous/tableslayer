    "@tableslayer/ui": "workspace:*",
    "@tanstack/svelte-query": "^5.66.9",
    "@tursodatabase/api": "^1.9.0",
    "@types/chroma-js": "^3.1.1",
    "axios": "^1.7.9",
    "chroma-js": "^3.1.2",
    "dotenv": "^16.4.7",
