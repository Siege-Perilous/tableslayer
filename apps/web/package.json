{
  "name": "web",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev-local": "vite dev --host 0.0.0.0 --port 8080",
    "build": "svelte-kit sync && vite build",
    "preview": "vite preview",
    "start": "node build",
    "tsc": "svelte-kit sync && tsc",
    "test": "pnpm run test:integration && pnpm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "devwebhooks": "smee -u https://smee.io/8cXKVPZb3wYvKCn -t http://localhost:5174/api/lemonsqueezy/webhook",
    "lint": "eslint .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "generate": "drizzle-kit generate --config ./src/lib/db/app/drizzle.config.app.ts",
    "migrate": "drizzle-kit migrate --config ./src/lib/db/app/drizzle.config.app.ts",
    "push": "drizzle-kit push --config ./src/lib/db/app/drizzle.config.app.ts",
    "drop": "drizzle-kit drop --config ./src/lib/db/app/drizzle.config.app.ts",
    "pull": "drizzle-kit pull --config ./src/lib/db/app/drizzle.config.app.ts",
    "studio": "drizzle-kit studio --config ./src/lib/db/app/drizzle.config.app.ts",
    "postinstall": "svelte-kit sync"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "=3.712.0",
    "@aws-sdk/s3-request-presigner": "=3.712.0",
    "@libsql/client": "^0.15.9",
    "@melt-ui/pp": "^0.3.2",
    "@node-rs/argon2": "^2.0.2",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@sentry/sveltekit": "^9.30.0",
    "@tabler/icons-svelte": "^3.34.0",
    "@tableslayer/ui": "workspace:*",
    "@tanstack/svelte-query": "^5.81.2",
    "@tursodatabase/api": "^1.9.0",
    "@types/ws": "^8.18.1",
    "chroma-js": "^3.1.2",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.2",
    "drizzle-zod": "^0.8.2",
    "lib0": "^0.2.109",
    "mime": "^4.0.7",
    "mode-watcher": "^1.0.8",
    "paneforge": "^1.0.0",
    "resend": "^4.6.0",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^18.2.1",
    "uuid": "^11.1.0",
    "vite-sveltekit-node-ws": "^0.0.8",
    "ws": "^8.18.2",
    "y-protocols": "^1.0.6",
    "y-socket.io": "^1.1.3",
    "yjs": "^13.6.27",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.1",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "5.1.0",
    "@tableslayer/eslint-config": "workspace:*",
    "@tableslayer/tsconfig": "workspace:*",
    "@types/chroma-js": "^3.1.1",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.31.1",
    "lucia": "^3.2.2",
    "postprocessing": "6.37.4",
    "smee-client": "^4.3.1",
    "svelte": "5.34.7",
    "svelte-check": "^4.2.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
