{
  "name": "web",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build",
    "preview": "vite preview",
    "start": "node build",
    "tsc": "svelte-kit sync && tsc",
    "test": "pnpm run test:integration && pnpm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "devwebhooks": "smee -u https://smee.io/8cXKVPZb3wYvKCn -t http://localhost:5174/api/lemonsqueezy/webhook",
    "lint": "eslint .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "generate": "drizzle-kit generate --config ./src/lib/db/app/drizzle.config.app.ts",
    "migrate": "drizzle-kit migrate --config ./src/lib/db/app/drizzle.config.app.ts",
    "push": "drizzle-kit push --config ./src/lib/db/app/drizzle.config.app.ts",
    "drop": "drizzle-kit drop --config ./src/lib/db/app/drizzle.config.app.ts",
    "pull": "drizzle-kit pull --config ./src/lib/db/app/drizzle.config.app.ts",
    "studio": "drizzle-kit studio --config ./src/lib/db/app/drizzle.config.app.ts",
    "postinstall": "svelte-kit sync"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "=3.712.0",
    "@aws-sdk/s3-request-presigner": "=3.712.0",
    "@libsql/client": "^0.15.2",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@melt-ui/pp": "^0.3.2",
    "@node-rs/argon2": "^2.0.2",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@sentry/sveltekit": "^9",
    "@tabler/icons-svelte": "^3.31.0",
    "@tableslayer/ui": "workspace:*",
    "@tanstack/svelte-query": "^5.71.10",
    "@tursodatabase/api": "^1.9.0",
    "chroma-js": "^3.1.2",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.41.0",
    "drizzle-zod": "^0.7.1",
    "mime": "^4.0.7",
    "mode-watcher": "^0.5.1",
    "paneforge": "^0.0.6",
    "resend": "^4.2.0",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^18.0.0",
    "uuid": "^11.1.0",
    "vite-sveltekit-node-ws": "^0.0.8",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/kit": "^2.20.4",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "@tableslayer/eslint-config": "workspace:*",
    "@tableslayer/tsconfig": "workspace:*",
    "@types/chroma-js": "^3.1.1",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.30.6",
    "lucia": "^3.2.2",
    "postprocessing": "6.37.2",
    "smee-client": "^3.1.1",
    "svelte": "5.25.7",
    "svelte-check": "^4.1.5",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^6.2.5",
    "vitest": "^3.1.1"
