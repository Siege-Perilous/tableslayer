{
  "name": "web",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev & partykit dev",
    "dev:vite": "vite dev",
    "dev:party": "partykit dev",
    "dev-local": "vite dev --host 0.0.0.0 --port 8080",
    "build": "svelte-kit sync && vite build",
    "build:party": "partykit build",
    "deploy:party": "partykit deploy",
    "preview": "vite preview",
    "start": "node build",
    "tsc": "svelte-kit sync && tsc",
    "test": "pnpm run test:unit && pnpm run test:integration",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "devwebhooks": "smee -u https://smee.io/8cXKVPZb3wYvKCn -t http://localhost:5174/api/lemonsqueezy/webhook",
    "lint": "eslint .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "generate": "drizzle-kit generate --config ./src/lib/db/app/drizzle.config.app.ts",
    "migrate": "drizzle-kit migrate --config ./src/lib/db/app/drizzle.config.app.ts",
    "push": "drizzle-kit push --config ./src/lib/db/app/drizzle.config.app.ts",
    "drop": "drizzle-kit drop --config ./src/lib/db/app/drizzle.config.app.ts",
    "pull": "drizzle-kit pull --config ./src/lib/db/app/drizzle.config.app.ts",
    "studio": "drizzle-kit studio --config ./src/lib/db/app/drizzle.config.app.ts",
    "postinstall": "svelte-kit sync"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "=3.712.0",
    "@aws-sdk/s3-request-presigner": "=3.712.0",
    "@libsql/client": "^0.15.15",
    "@melt-ui/pp": "^0.3.2",
    "@node-rs/argon2": "^2.0.2",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@sentry/sveltekit": "^10.15.0",
    "@tabler/icons-svelte": "^3.35.0",
    "@tableslayer/ui": "workspace:*",
    "@tanstack/svelte-query": "^5.90.2",
    "@tursodatabase/api": "^1.9.0",
    "@types/marked": "^6.0.0",
    "@types/ws": "^8.18.1",
    "arctic": "^3.7.0",
    "chroma-js": "^3.1.2",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "drizzle-zod": "^0.8.3",
    "lib0": "^0.2.114",
    "marked": "^16.3.0",
    "mime": "^4.1.0",
    "mode-watcher": "^1.1.0",
    "paneforge": "^1.0.2",
    "partykit": "^0.0.115",
    "resend": "^6.1.0",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^18.5.0",
    "uuid": "^13.0.0",
    "vite-sveltekit-node-ws": "^0.0.8",
    "ws": "^8.18.3",
    "y-partykit": "^0.0.33",
    "y-protocols": "^1.0.6",
    "y-socket.io": "^1.1.3",
    "yjs": "^13.6.27",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/adapter-node": "^5.3.2",
    "@sveltejs/kit": "^2.43.5",
    "@sveltejs/vite-plugin-svelte": "6.2.1",
    "@tableslayer/eslint-config": "workspace:*",
    "@tableslayer/tsconfig": "workspace:*",
    "@types/chroma-js": "^3.1.1",
    "@types/uuid": "^11.0.0",
    "drizzle-kit": "^0.31.5",
    "lucia": "^3.2.2",
    "postprocessing": "6.37.8",
    "smee-client": "^4.3.1",
    "svelte": "5.39.6",
    "svelte-check": "^4.3.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  }
}
