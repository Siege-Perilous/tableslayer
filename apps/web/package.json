{
  "name": "web",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build",
    "preview": "vite preview",
    "start": "node build",
    "tsc": "svelte-kit sync && tsc",
    "test": "pnpm run test:integration && pnpm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "devwebhooks": "smee -u https://smee.io/8cXKVPZb3wYvKCn -t http://localhost:5174/api/lemonsqueezy/webhook",
    "lint": "eslint .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "appDb:generate": "drizzle-kit generate --config ./src/lib/db/app/drizzle.config.app.ts",
    "appDb:migrate": "drizzle-kit migrate --config ./src/lib/db/app/drizzle.config.app.ts",
    "appDb:push": "drizzle-kit push --config ./src/lib/db/app/drizzle.config.app.ts",
    "appDb:drop": "drizzle-kit drop --config ./src/lib/db/app/drizzle.config.app.ts",
    "appDb:pull": "drizzle-kit pull --config ./src/lib/db/app/drizzle.config.app.ts",
    "appDb:studio": "drizzle-kit studio --config ./src/lib/db/app/drizzle.config.app.ts",
    "gsDb:generate": "drizzle-kit generate --config ./src/lib/db/gs/drizzle.config.gs.ts",
    "gsDb:migrate": "drizzle-kit migrate --config ./src/lib/db/gs/drizzle.config.gs.ts",
    "gsDb:push": "drizzle-kit push --config ./src/lib/db/gs/drizzle.config.gs.ts",
    "gsDb:pull": "drizzle-kit pull --config ./src/lib/db/gs/drizzle.config.gs.ts",
    "gsDb:drop": "drizzle-kit drop --config ./src/lib/db/gs/drizzle.config.gs.ts",
    "gsDb:studio": "drizzle-kit studio --config ./src/lib/db/gs/drizzle.config.gs.ts",
    "postinstall": "svelte-kit sync"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "=3.712.0",
    "@aws-sdk/s3-request-presigner": "=3.712.0",
    "@lemonsqueezy/lemonsqueezy.js": "^4.0.0",
    "@libsql/client": "^0.14.0",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@melt-ui/pp": "^0.3.2",
    "@node-rs/argon2": "^2.0.2",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@sentry/sveltekit": "^8",
    "@tabler/icons-svelte": "^3.29.0",
    "@tableslayer/ui": "workspace:*",
    "@tanstack/svelte-query": "^5.66.0",
    "@tursodatabase/api": "^1.9.0",
    "@types/chroma-js": "^3.1.1",
    "axios": "^1.7.9",
    "chroma-js": "^3.1.2",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.39.1",
    "drizzle-zod": "^0.7.0",
    "mime": "^4.0.6",
    "mime-types": "^2.1.35",
    "mode-watcher": "^0.5.1",
    "paneforge": "^0.0.6",
    "resend": "^4.1.1",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.0.5",
    "vite-sveltekit-node-ws": "^0.0.8",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/adapter-vercel": "^5.5.3",
    "@sveltejs/kit": "^2.16.1",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "@tableslayer/eslint-config": "workspace:*",
    "@tableslayer/tsconfig": "workspace:*",
    "@types/mime-types": "^2.1.4",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.30.4",
    "lucia": "^3.2.2",
    "postprocessing": "^6.36.6",
    "smee-client": "^2.0.4",
    "svelte": "5.19.6",
    "svelte-check": "^4.1.4",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vitest": "^3.0.4"
  }
}
